{
    "name": "smarttesting/05-02-packages",
    "type": "project",
    "authors": [
        {
            "name": "Arkadiusz Kondas",
            "email": "arkadiusz.kondas@gmail.com"
        }
    ],
    "require": {
        "psr/log": "^1.1",
        "symfony/http-client": "^5.1",
        "symfony/uid": "^5.1"
    },
    "require-dev": {
        "phpat/phpat": "^0.8",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
